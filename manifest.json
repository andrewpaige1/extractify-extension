{
  "manifest_version": 3,
  "name": "Extractify",
  "description": "Extracts terms and definitions from a PowerPoint file",
  "version": "0.0.0.4",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "ExtractifyB.png"
  },
  "icons": {
    "16": "ExtractifyB.png",
    "48": "ExtractifyB.png",
    "128": "ExtractifyB.png"
  },
  "host_permissions": ["https://auto-text.onrender.com/", "https://auto-libs.onrender.com/"]
}
